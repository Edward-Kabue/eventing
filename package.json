{
  "name": "eventing",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:migrate": "npm run build && sequelize-cli db:migrate",
    "db:migrate:undo": "npm run build && sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npm run build && sequelize-cli db:migrate:undo:all",
    "db:seed:all": "npm run build && sequelize-cli db:seed:all",
    "db:seed:undo:all": "npm run build && sequelize-cli db:seed:undo:all",
    "db:create": "npm run build && sequelize-cli db:create",
    "db:drop": "npm run build && sequelize-cli db:drop",
    "migration:generate": "npm run build && sequelize-cli migration:generate --name",
    "seed:generate": "npm run build && sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "@types/bcryptjs": "^2.4.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  }
}
